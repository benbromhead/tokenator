<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg - %X%n</pattern>
        </encoder>
    </appender>
    <root level="OFF">
        <appender-ref ref="STDOUT" />
    </root>
    <logger name="com.instaclustr.tokenator" level="INFO" />
</configuration>
